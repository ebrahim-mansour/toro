extends layout
block title
  title Toro
block style
  style.
    .dropdown .dropbtn {
      font-size: 13px;  
      border: none;
      color: white;
      background-color: inherit;
      text-transform: uppercase;
      letter-spacing: 2px;
    }
    .navbar a:hover, .dropdown:hover .dropbtn {
      color: #e5ce48;
    }
    .dropdown-content {
      display: none;
      position: absolute;
      background-color: #f9f9f9;
      min-width: 160px;
      box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
      z-index: 1;
    }
    .dropdown-content a {
      color: black;
      padding: 6px 0px;
      text-decoration: none;
      display: block;
      text-align: center;
    }
    .dropdown-content a:hover {
      background-color: #ddd;
    }
    .dropdown:hover .dropdown-content {
      display: block;
    }
